/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=c);
    Nand(a=a, b=c, out=c0);
    Nand(a=b, b=c, out=c1);
    Nand(a=c0, b=c1, out=out);
}